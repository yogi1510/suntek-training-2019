#include <stdio.h>
#include <stdlib.h>

int main()
{
   int a[10][10],b[10],n,i,j;
   printf("enter size");
   scanf("%d",&n);
   printf("enter elements");
   for(i=0;i<n;i++)
   {
       for(j=0;j<n;j++)
        scanf("%d",&a[i][j]);
   }
    for(i=0;i<n;i++)
   {
       for(j=0;j<n;j++)
       {
           if(i==j)
           b[i]=a[i][j];

       }

   }
   for(i=0;i<n;i++)
    printf("%d ",b[i]);



    return 0;
}
